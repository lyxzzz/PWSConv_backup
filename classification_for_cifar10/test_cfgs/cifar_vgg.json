{
    "gpuid":0,
    "test_parameters":{
        "ckpt_path":"checkpoints",
        "ckpt_name":"tpn_last.ckpt",

        "write_to_file":false,
        "anno_path":"anno_test/ssd",
        "matching_threhold":0.5,
        "test_batch_nums":256,
        "test_thread_nums":16,
        "test_queue_size":16
