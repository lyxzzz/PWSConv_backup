        "header_norm":true,
        "backbone_norm":true,
        "conv_type":"PWS",
        "pwsepsilon":0.001
    },

    "backbone":{
        "type":"cifar_VGG"
    },

    "augmentation":{
        "size":[32, 32],
        "type":"cifar10",
        "parameters":{},
        "dataset_distribution":{
            "mean":[125.0, 122.0, 113.0],
            "var":[51.0, 52.0, 51.0]
        }
    }
