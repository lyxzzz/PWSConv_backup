{
    "gpuid":0,
    "test_parameters":{
        "ckpt_path":"checkpoints/ssd/ssd_300_vgg",
        "ckpt_path":"checkpoints/stdn",
        "ckpt_name":"stdn_last.ckpt",
        "write_to_file":false,
        "anno_path":"anno_test/stdn",
        "matching_threhold":0.5,
        "test_batch_nums":32,
        "test_thread_nums":8,
        "test_queue_size":16
    },
    "dataset":{
        "test":["VOC07"]
    },
    "default_network_cfgs":"STDN",

    "network":{
        "weight_decay":0.0005,
        "dropout_keep_prob":0.5,
        "norm_func":"BatchNorm",
        "batch_norm":{
            "decay":0.997,
            "epsilon":0.001
        },
        "stop_gradient":false
    },

    "backbone":{
        "type":"DenseNet",
        "bottleneck_ratio":4,
        "growth_num":32,
        "transition_ratio":0.5,
        "dense_block":[6, 12, 32, 32],
        "use_stem":true
    },
    "header":{
        "type":"STDN",
        "head_channels":256,
        "last_layer_norm":"None"
    },
    "postprocessing":{
        "type":"ssdnms",
        "select_threshold":0.01,
        "nms_threshold":0.5,
        "top_k":400,
        "keep_top_k":200
    }
}